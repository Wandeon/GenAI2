# ============================================================================
# GenAI Observatory - Environment Variables
# ============================================================================
# Copy this file to .env and fill in the values

# Database (PostgreSQL)
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/genai_observatory"

# Redis (for BullMQ)
REDIS_URL="redis://localhost:6379"

# API
PORT=4000
HOST=0.0.0.0
CORS_ORIGIN="http://localhost:3000"
COOKIE_SECRET="change-this-in-production"

# LLM Providers
GOOGLE_AI_API_KEY=""
OPENAI_API_KEY=""
ANTHROPIC_API_KEY=""

# Node environment
NODE_ENV="development"
